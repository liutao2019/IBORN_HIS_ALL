<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="dsDayReport" targetNamespace="http://tempuri.org/dsDayReport.xsd" elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/dsDayReport.xsd" xmlns:mstns="http://tempuri.org/dsDayReport.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xs:element name="dsDayReport" msdata:IsDataSet="true">
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">
                <xs:element name="dayReport">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="处方开始号" type="xs:string" minOccurs="0" />
                            <xs:element name="处方结束号" type="xs:string" minOccurs="0" />
                            <xs:element name="处方张数" type="xs:integer" minOccurs="0" />
                            <xs:element name="挂号金额" type="xs:decimal" minOccurs="0" />
                            <xs:element name="诊金" type="xs:decimal" minOccurs="0" />
                            <xs:element name="病历金额" type="xs:decimal" minOccurs="0" />
                            <xs:element name="其他金额" type="xs:decimal" minOccurs="0" />
                            <xs:element name="实收金额" type="xs:decimal" minOccurs="0" />
                            <xs:element name="状态" type="xs:string" minOccurs="0" />
                            <xs:element name="操作员代码" type="xs:string" minOccurs="0" />
                            <xs:element name="操作员名称" type="xs:string" minOccurs="0" />
                            <xs:element name="开始时间" type="xs:dateTime" />
                            <xs:element name="结束时间" type="xs:dateTime" minOccurs="0" />
                            <xs:element name="操作时间" type="xs:dateTime" minOccurs="0" />
                            <xs:element name="医院名称" type="xs:string" minOccurs="0" />
                            <xs:element name="实收合计" type="xs:decimal" minOccurs="0" />
                            <xs:element name="挂号合计" type="xs:decimal" minOccurs="0" />
                            <xs:element name="诊金合计" type="xs:decimal" minOccurs="0" />
                            <xs:element name="大写金额" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:complexType>
    </xs:element>
</xs:schema>