				        <Sql id="DeptPaList">
select pt.id,
       (select t.ops_mode_name
          from CASE_OPS_RECORD_SJ t
         where t.id =
               (select max(id)
                  from CASE_OPS_RECORD_SJ t
                 where t.patient_id = (select cu.patient_id
                                         from pt_inpatient_cure cu
                                        where cu.id = pt.id))) as ops_name,
       (select t.ops_time
          from CASE_OPS_RECORD_SJ t
         where t.id =
               (select max(id)
                  from CASE_OPS_RECORD_SJ t
                 where t.patient_id = (select cu.patient_id
                                         from pt_inpatient_cure cu
                                        where cu.id = pt.id))) as ops_time,
(select t.balance_date from his_kfc.FIN_IPR_INMAININFO t where t.inpatient_no = pt.inpatient_no) as balance_date
  from vhis_inpatientinfo pt
 where pt.id in ({0})
				        </Sql>
