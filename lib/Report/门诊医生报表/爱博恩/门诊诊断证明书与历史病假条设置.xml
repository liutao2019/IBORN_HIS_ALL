rg.card_no 门诊号,
rg.name 患者姓名,
decode(rg.sex_code,'F','女','男') 性别,
TRUNC(months_between(sysdate, rg.birthday)/12) 年龄,
rg.see_date 就诊日期,
rg.dept_name 就诊科室,
rg.doct_name 看诊医师,
di.prove_no 诊断证明书序号,
(select wm_concat(dg.diag_name)  from met_cas_diagnose dg where dg.valid_flag = '1' and dg.inpatient_no = rg.clinic_code )  诊断,
ca.ext_flag1 治疗意见,
di.prove_printdate 诊断证明打印时间,
di.leave_no 病假建议书序号,
di.leave_days 病假天数,
to_char(di.leave_start,'yyyy-MM-dd') 病假开始时间,
to_char(di.leave_end,'yyyy-MM-dd') 病假结束时间,
di.leave_printdate 病假建议书打印时间
from fin_opr_register rg
left join MET_CAS_HISTORY ca on rg.clinic_code = ca.clinic_code
left join MET_CAS_DIAGEXTEND di on rg.clinic_code = di.clinic_code
where 1=1
and rg.valid_flag = '1'
and di.valid_flag = '1' 
and rg.reg_date between to_date('&amp;dtBeginTime', 'YYYY-MM-DD') and to_date('&amp;dtEndTime', 'YYYY-MM-DD') + 1
and (rg.dept_code = '&amp;cmbDept' OR '&amp;cmbDept' = 'ALL')
and (rg.name like '%&amp;dtPatient%' OR rg.card_no like '%&amp;dtPatient%' )
and (rg.doct_code = '&amp;cmbDoct' OR '&amp;cmbDoct' = 'ALL')
</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>true</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\门诊医生报表\爱博恩\门诊诊断证明书与历史病假条设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>