LikeStr>%{0}%</LikeStr><Size Type="System.Drawing.Size,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Width>120</Width><Height>20</Height></Size><Location Type="System.Drawing.Point,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><X>660</X><Y>5</Y></Location><QueryDataSource>Dictionary</QueryDataSource><DataSourceTypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dtData</Name><Sql>select r.name,
       r.pact_name,
       r.inpatient_no,
       r.patient_no,
       r.in_times,
       (select s.mcard_no from fin_ipr_siinmaininfo s where s.inpatient_no = r.inpatient_no and rownum = 1) mcard_no,
       (select s.reg_no from fin_ipr_siinmaininfo s where s.inpatient_no = r.inpatient_no and rownum = 1) reg_no,
       r.idenno,
         to_char(r.in_date,'yyyy-mm-dd hh24:mi:ss'),
       (case when r.out_date &lt; to_date('1900','yyyy') then null else to_char(r.out_date,'yyyy-mm-dd') end) out_date,
       (case when r.out_date - r.in_date &gt; 0  then ceil(r.out_date - r.in_date) else 0 end) days,
       r.dept_name,
       substr(r.bed_no,5),
       decode(r.in_state, 'R', '住院登记', 'I', '在院', 'B', '出院登记', 'O', '出院结算', 'P', '预约出院', 'N', '无费退院', 'C', '封帐') in_state,
       decode(r.sex_code, 'F', '女', 'M', '男', '未知') sex,
       fun_get_age(r.birthday) age,
       r.work_name,
       '',
       (r.HOME_TEL||'/'||r.WORK_TEL||'/'||r.LINKMAN_TEL) tel,
       r.clinic_diagnose
  from fin_ipr_inmaininfo r
 where ('False' = '&amp;dtIsInDate' or (r.in_date &gt;= to_date('&amp;dtInBeginTime','yyyy-mm-dd hh24:mi:ss') and r.in_date &lt;= to_date('&amp;dtInEndTime','yyyy-mm-dd hh24:mi:ss')))
   and ('False' = '&amp;dtIsOutDate' or (r.out_date &gt;= to_date('&amp;dtOutBeginTime','yyyy-mm-dd hh24:mi:ss') and r.out_date &lt;= to_date('&amp;dtOutEndTime','yyyy-mm-dd hh24:mi:ss')))
   and r.dept_code in('&amp;dtDept.Values')
   AND ('ALL'='&amp;dtInState' OR r.IN_STATE = '&amp;dtInState')
   AND ('ALL' = '&amp;dtPactInfo' OR r.PACT_CODE = '&amp;dtPactInfo')
   AND ('ALL' = decode(trim('&amp;txtPatientNO'),'','ALL',trim('&amp;txtPatientNO')) OR r.PATIENT_NO LIKE '%&amp;txtPatientNO%' OR r.NAME LIKE '%&amp;txtPatientNO%' OR substr(r.bed_no,5) LIKE  '%&amp;txtPatientNO'  )

</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>\Report\住院收费报表\病人一览表查询设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>