select      a.apply_no 申请流水号,   --申请流水号
            a.apply_list_code 申请单号,   --申请单号
            a.apply_serial_no 单内序号,   --单内序号
            fun_get_dept_name(a.storage_code) 申请部门,   --申请部门(库存部门)
            fun_get_dept_name(a.target_dept) 目标科室,   --目标科室
            decode(a.apply_state,'0','申请','1','审批','2','汇总','3','核准','5','作废','其他') 申请状态,   --申请状态(0-申请,1-审批,3-核准)
            a.item_code 物品编码,   --物品编码
            b.custom_code 物品自定义码, --自定义码
            a.item_name 物品名称,   --物品名称
            a.specs 规格,   --规格
            a.apply_num 最小单位申请数量,   --申请数量
            a.min_unit 最小单位,   --最小单位
            fun_get_employee_name(a.apply_oper) 申请人,   --申请人
            round(a.apply_num/a.pack_qty,5) 大包装申请数量,   --大包装包装数量
            a.pack_unit 大包装单位,   --大包装单位taehrt4h
            a.apply_date 申请时间,   --申请时间
            fun_get_employee_name(a.exam_oper) 审批人,   --审批人
            a.exam_date 审批人时间,   --审批人时间
            a.exam_num 最小单位审批数量,   --审批数量
            a.min_unit 最小单位,   --最小单位
            a.memo 备注,   --备注
            case when a.out_num=0 then '否' else '是' end 是否出库,--是否出库
            a.out_num 已经出库数量 -- 已经出库数量
 from mat_com_applyforoutput a 
 left join mat_com_baseinfo b on a.item_code=b.item_code
 where a.apply_date &gt; to_date('&amp;dtBeginTime','yyyy-MM-dd hh24:mi:ss') 
 and a.apply_date &lt; to_date('&amp;dtEndTime','yyyy-MM-dd hh24:mi:ss')+1 
 and (a.storage_code='&amp;DeptName' or  'ALL'='&amp;DeptName' )
  and (a.item_code = '&amp;ItemName' or  '&amp;ItemName'='ALL')
and a.target_dept='&amp;dtDept'
order by  a.apply_date  desc
</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\科室月计划报表.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>