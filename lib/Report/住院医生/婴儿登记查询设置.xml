        fun_get_employee_name(cp.oper_code) 登记人,
        bb.height 身高,
        bb.weight 体重,
        bb.delivery_name 分娩方式,
        bb.birthday 出生时间,
        cp.oper_date 登记时间,
        cd1.dept_name 科室,
        fi.nurse_cell_name 病区
   from com_shiftdata cp 
   left join fin_ipr_babyinfo bb on cp.clinic_no=bb.inpatient_no
   left join com_employee ce on bb.oper_code=ce.empl_code
   left join fin_ipr_inmaininfo fi on bb.mother_inpatient_no=fi.inpatient_no
   left join com_department cd on fi.nurse_cell_code=cd.dept_code
   left join com_department cd1 on cd.dept_ename=cd1.dept_code
  where cp.oper_date between to_date('&amp;dtBeginTime' ,'yyyy-MM-dd')  and to_date( '&amp;dtEndTime','yyyy-MM-dd') +1 
    and nvl(bb.cancel_flag,'')&lt;&gt;1 and fi.dept_code='1001'
    and cp.shift_type='K'
    and (cp.oper_code='&amp;empl_code' or '&amp;empl_code'='ALL')
    and (bb.delivery_code='&amp;delivery_code' or '&amp;delivery_code'='ALL')
    and (fi.nurse_cell_code='&amp;nurse_cell_code' or '&amp;nurse_cell_code'='ALL')
    and (cd1.dept_code='&amp;dept_code' or '&amp;dept_code'='ALL')
  order by cp.oper_date
  </Sql><AddMapRow>false</AddMapRow><AddMapColumn>true</AddMapColumn><AddMapData>true</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\住院医生\婴儿登记查询设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>