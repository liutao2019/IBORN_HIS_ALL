          t.card_no 病历号, 
          (select a.name from com_patientinfo a where a.card_no = t.card_no) 患者姓名,
          (select a.type_name from bd_com_accounttype a where a.type_code = t.type_code) 账户类型,
          case cancel_flag when '1' then '收取' when '0' then '返还' when '2' then '重打' else '未知' end 操作类型,
          (select a.name from com_dictionary a where a.code = t.prepay_type and a.type = 'PAYMODES') 支付方式,
          t.prepay_cost 充值金额,
          t.donate_cost 赠送金额,
          (select a.empl_name from com_employee a where a.empl_code = t.oper_code) 操作员,
(select a.remark from EXP_CASHCOUPON_RECORD a where a.invoice_no= t.invoice_no) 充值备注,

          t.oper_date 操作日期
 from fin_opb_accountprepay t
 where t.oper_date between to_date('&amp;dtBeginTime','yyyy-mm-dd') and to_date('&amp;dtEndTime','yyyy-mm-dd')+1
   and (t.oper_code = '&amp;cmbFeeOper' or '&amp;cmbFeeOper' = 'ALL')
   and ((select a.name from com_patientinfo a where a.card_no = t.card_no) like '%&amp;txtName%' or '&amp;txtName' is null)
   and (substr(t.invoice_no,1,3) = '&amp;cmbHospital' or '&amp;cmbHospital' = 'ALL')
order by invoice_no</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\门诊财务报表\爱博恩\账户充值统计设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>