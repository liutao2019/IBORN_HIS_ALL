select apply_no,   --申请流水号
            apply_list_code,   --申请单号
            apply_serial_no,   --单内序号
            fun_get_dept_name(storage_code),   --申请部门(库存部门)
            fun_get_dept_name(target_dept),   --目标科室
            decode(apply_state,'0','申请','1','审批','2','汇总','3','核准','5','作废','其他'),   --申请状态(0-申请,1-审批,3-核准)
            item_code,   --物品编码
            item_name,   --物品名称
            specs,   --规格
            min_unit,   --最小单位
            pack_unit,   --大包装单位
            pack_qty,   --大包装包装数量
            ROUND(apply_num/pack_qty,2),   --申请数量
            fun_get_employee_name(apply_oper),   --申请人
            apply_date,   --申请时间
            fun_get_employee_name(exam_oper),   --审批人
            exam_date,   --审批人时间
            ROUND(exam_num/pack_qty,2),   --审批数量
            memo   --备注
 from mat_com_apply a 
 where a.apply_date &gt; to_date('&amp;dtBeginTime','yyyy-MM-dd hh24:mi:ss') 
 and a.apply_date &lt; to_date('&amp;dtEndTime','yyyy-MM-dd hh24:mi:ss') 
 and (a.storage_code='&amp;DeptName' or  'ALL'='&amp;DeptName' )
  and a.item_name like '&amp;ItemName'
