urce><DataSourceTypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dataDetail</Name><Sql>

select z.balance_item_name 结算名称,
       s.name 姓名,
			 s.idenno 身份证,
			 fun_get_age_new(i.birthday,i.in_date) 年龄,
			 decode(s.sex_code,'F','女','M','男','未知') 性别,
			 i.patient_no 住院号,
			 i.in_date 入院日期,
			 i.out_date 出院日期,
			 z.diagn1 诊断,
			 S.TOT_COST 实际医疗费,
			 z.approval_cost 核准医疗费,
			 s.own_cost 自费,
			 z.pay_rate 个人按比例自付,
			  '???' 结算定额,
			 s.pub_cost 基金支付金额,
			 '???' 超支结余,
			 '???' 按医保项目结算的医疗费用

from fin_ipr_siinmaininfo_zhuhai z ,fin_ipr_siinmaininfo s ,fin_ipr_inmaininfo i
where z.sy_flag='1'
and s.valid_flag='1'
and z.balance_flag='1'
and z.inpatient_no=s.inpatient_no
and z.balance_no=s.balance_no
and s.type_code='2'
and z.inpatient_no=i.inpatient_no
and i.out_date between to_date('&amp;dtBeginTime','YYYY-MM-dd hh24:mi:ss') and to_date('&amp;dtEndTime','YYYY-MM-dd hh24:mi:ss')
order by z.balance_item_name</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>\Report\公费报表\珠海生育保险住院明细设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>