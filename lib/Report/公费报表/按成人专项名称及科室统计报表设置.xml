urce><DataSourceTypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dataDetail</Name><Sql>select z.balance_item_name 专项名称,
        fun_get_dept_name(i.dept_code) 科室,
				nvl(c.item_cost,'未维护') 专项定额,
       count(distinct i.inpatient_no)  人次,
       decode(sign(sum(z.tot_cost)-sum(c.item_cost)),1,'(+)',-1,'(-)',0,'平','存在未维护') 超支节余合计,
       round((sum(z.tot_cost)-sum(c.item_cost))/count(distinct i.inpatient_no),2) 人均超支节余费用,
			 sum(z.tot_cost) 实际医疗费
from fin_ipr_inmaininfo i ,fin_ipr_siinmaininfo s ,fin_ipr_siinmaininfo_zhuhai z
     left join (select * from zhuhai_balance_type_cost ) c on c.si_type=z.si_type and c.balance_type=z.balance_type and c.item_code=z.balance_item
where i.inpatient_no =s.inpatient_no
and z.inpatient_no=s.inpatient_no
and z.balance_no=s.balance_no
and s.valid_flag='1' --有效
and s.type_code='2' --住院
and z.si_type in (2,3) --险种 1 未成年
and z.balance_type='01' --专项定额
and s.balance_date between to_date('&amp;dtBeginTime','YYYY-MM-dd hh24:mi:ss') and to_date('&amp;dtEndTime','YYYY-MM-dd hh24:mi:ss')
group by z.balance_item_name ,
        fun_get_dept_name(i.dept_code) ,
        nvl(c.item_cost,'未维护') 
</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>\Report\公费报表\按成人专项名称及科室统计报表设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>