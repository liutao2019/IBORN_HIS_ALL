dth>120</Width><Height>20</Height></Size><Location Type="System.Drawing.Point,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><X>1060</X><Y>2</Y></Location><QueryDataSource>Sql</QueryDataSource><DataSourceTypeName>select distinct a.channel2code,a.channel2,'','' from com_patientinfo a where a.channel2code is not null</DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dtpackagecharge</Name><Sql>select aa.yq, 
aa.card_no,
aa.patient_name,
case when aa.package_kind='儿保套餐' then aa.child_tel else aa.tel end tel,
aa.package_kind,
wm_concat(aa.packagename),
sum(aa.package_cost),sum(aa.real_cost),sum(aa.gift_cost),sum(aa.etc_cost)
,aa.oper_date
,cp.channel1
,cp.channel2
from (
select ( select a.hospital_name from com_department a where a.dept_code = (select b.dept_code from com_employee b where b.empl_code = t.oper_code)) yq,
          card_no,
          patient_name,
         （select b.home_tel from com_patientinfo b where b.card_no=t.card_no ） tel,
          (select '0'||b1.linkman_tel from com_patientinfo b1 where b1.card_no=t.card_no ) child_tel,
          (case when t.cancel_flag = '0' and t.trans_type = '1' then t.invoice_no
               when t.cancel_flag = '1' and t.trans_type = '1' then '[退费]'||t.invoice_no
               when t.cancel_flag = '1' and t.trans_type = '2' then '[退费]'||t.invoice_no
               else '[未知]'||t.invoice_no  end) invoice_no,
          (select y.name from com_dictionary y where y.type='PACKAGETYPE'and y.code=（select h.package_kind from bd_com_package h where h.package_id=t.package_id）)package_kind,        
          ( select a.package_name from bd_com_package a where a.package_id = t.package_id) packagename,
          package_cost,
          real_cost,
          gift_cost,
          etc_cost,
          to_char(oper_date,'yyyy-mm-dd') oper_date
 from exp_package t
 where pay_flag = '1'
   and t.oper_date between to_date('&amp;dtBeginTime','yyyy-mm-dd') and to_date('&amp;dtEndTime','yyyy-mm-dd')+1
      and (t.oper_code = '&amp;cmbFeeOper' or '&amp;cmbFeeOper' = 'ALL')
      and t.oper_code in (select empl_code from com_employee where empl_type = 'F')
      and (( select a.hospital_id from com_department a where a.dept_code = (select b.dept_code from com_employee b where b.empl_code = t.oper_code))  = '&amp;cmbHospital' or '&amp;cmbHospital' = 'ALL')
      and (nvl(length(t.memo),0) = 0  or (nvl(length(t.memo),0) &gt; 0 and t.memo != 'TCBL' ))
      and not exists (select * from exp_package x where t.clinic_code=x.clinic_code and t.card_no=x.card_no and t.package_id=x.package_id 
      and t.pay_flag=1 and x.pay_flag=1 and t.real_cost=-x.real_cost and abs(t.real_cost)&gt;0)
   order by t.invoice_no,t.trans_type,t.clinic_code
   )aa
   left join com_patientinfo cp on cp.card_no=aa.card_no
   where (cp.channel1code='&amp;channel1code' or '&amp;channel1code'='ALL')
   and  (cp.channel1code='&amp;channel2code' or '&amp;channel2code'='ALL')
   group by  aa.yq, aa.card_no,aa.patient_name,case when aa.package_kind='儿保套餐' then aa.child_tel else aa.tel end,aa.package_kind,aa.oper_date
   ,cp.channel1,cp.channel2</Sql><AddMapRow>false</AddMapRow><AddMapColumn>true</AddMapColumn><AddMapData>true</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>\Report\门诊财务报表\爱博恩\套餐收费统计数据分析设置(新).xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>