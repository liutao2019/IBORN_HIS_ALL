ht></Size><Location Type="System.Drawing.Point,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><X>1060</X><Y>2</Y></Location><QueryDataSource>Dictionary</QueryDataSource><DataSourceTypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="Neusoft.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dSPackageSource</Name><Sql>
select 
j.hospital_name,
dd.card_no,
dd.PATIENT_NAME,
 dd.invoice_no,
com_dictionary.name, 
replace(wm_concat (aa.package_name),',','+'  ) tc_name ,
sum(dd.package_cost) package_cost ,
sum(dd.real_cost) real_cost ,
sum(dd.gift_cost) gift_cost ,
sum(dd.etc_cost) etc_cost,
dd.PATIENT_NAME||':'||pay_dt,
qq.mon,
g.empl_name op_name,
TO_CHAR(dd.oper_date,'YYYY-MM-DD'),
g1.empl_name  sv_name,
f.home_tel
from exp_package  dd
left join bd_com_package aa on aa.package_id =dd.package_id  
left join com_dictionary on code = aa.package_kind  and com_dictionary.type = 'PACKAGETYPE'  
left join com_patientinfo f on f.card_no = dd.card_no
left join com_employee g on g.empl_code = dd.oper_code
left join com_employee g1 on g1.empl_code = dd.Consultant
left join com_department j on j.dept_code = g.dept_code
left join 
(select  H.INVOICE_NO,wm_concat(cm_diy.NAME||':'||h.tot_cost||'')  pay_dt
from exp_packagepaymode  H 
left join com_dictionary  cm_diy on  H.MODE_CODE =cm_diy.CODE  and type = 'PAYMODES' 
where  H.INVOICE_NO not in (select INVOICE_NO from exp_packagepaymode where exp_packagepaymode.trans_type =2  )
group by H.INVOICE_NO ) paying_detail on paying_detail.INVOICE_NO = dd.invoice_no
left join
(select  invoice_no, '备注：'||EXP_PACKAGEINVOICE.memo mon from EXP_PACKAGEINVOICE where invoice_no not in
 (select invoice_no from EXP_PACKAGEINVOICE where trans_type = 2 ) ) qq on qq.invoice_no = dd.invoice_no  
where   dd.INVOICE_NO not in (select INVOICE_NO from exp_package where exp_package.trans_type =2  )
AND (DD.PATIENT_NAME like  '%&amp;txtname%' or '&amp;txtname' is null )
and ( j.hospital_name = '&amp;txthosname'  or '&amp;txthosname'   ='ALL')
and (g.empl_name = '&amp;opername' or  '&amp;opername'='ALL')
and (dd.card_no = '&amp;txtcard' or '&amp;txtcard' is null)
and  dd.oper_date between  TO_date('&amp;dtBeginTime','YYYY-MM-DD') and  TO_date('&amp;dtEndTime','YYYY-MM-DD') 
group by 
j.hospital_name,
dd.invoice_no,
dd.card_no,
dd.PATIENT_NAME,
com_dictionary.name,
dd.PATIENT_NAME||':'||pay_dt,
qq.mon,
g.empl_name,
TO_CHAR(dd.oper_date,'YYYY-MM-DD'),
f.home_tel,
g1.empl_name </Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\门诊财务报表\爱博恩\套餐收费查询设置(汇总).xml</QueryFilePath><PrintInfo Type="Neusoft.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>