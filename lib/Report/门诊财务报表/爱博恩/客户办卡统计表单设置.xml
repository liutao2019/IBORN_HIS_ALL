ypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dgCusInfo</Name><Sql>select name 姓名,createdate 建卡日期,phone 电话 
,sex 性别,to_char(birthday,'yyyy-mm-dd') 出生年月,fun_get_age_new1(birthday,sysdate) 年龄,lxr 联系人,lxrdh 联系人电话
from 
(
select aa.name name,
         case when createdate1 is null then createdate2
                   else createdate1 end createdate,
         aa.phone phone,
         case when aa.sex='M' then '男' when aa. sex='F' then '女' when aa.sex='1' then '男' when aa.sex='2' then '女' when aa.sex='3' then '保密' else '未知' end sex,
         aa.birthday,
         aa.lxr,
         aa.lxrdh
  from (select t.name name,
               (select min(a.createdate)
                  from fin_opb_accountcard a
                 where a.card_no = t.card_no) createdate1,
               t.oper_date createdate2,
               t.home_tel phone,
               t.sex_code sex,
               t.birthday birthday,
               t.linkman_name lxr,
               t.linkman_tel lxrdh
          from com_patientinfo t) aa
          )
          where createdate between to_date('&amp;dtBeginTime','yyyy-mm-dd') and to_date('&amp;dtEndTime','yyyy-mm-dd')+1
          
