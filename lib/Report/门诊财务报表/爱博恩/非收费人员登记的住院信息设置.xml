       fi.oper_date 登记时间,
       fi.name 客户姓名,
       to_char(fi.birthday,'yyyy-mm-dd') 出生年月日,
       fun_get_age(fi.birthday) 年龄,
       decode(fi.sex_code,'F','女','M','男','不明') 性别,
       fi.card_no 门诊号,
       fi.patient_no 住院号,
       fi.pact_name 结算类别,
       fi.dept_name 科室,
       fi.nurse_cell_name 病区,
       decode(fi.in_state,'R','住院登记','I','病房接诊','B','出院登记','O','出院结算','P','预约出院','N','无费退院','C','挂账') 在院状态
  from fin_ipr_inmaininfo fi 
  left join com_employee ce on fi.oper_code=ce.empl_code
  left join com_department cd on ce.dept_code=cd.dept_code
 where fi.oper_date between to_date('&amp;dtBeginTime' ,'yyyy-MM-dd')  and to_date( '&amp;dtEndTime','yyyy-MM-dd') +1 
   and (fi.name like '%&amp;name%' or '&amp;name' is null)
   and cd.dept_name not like '%收费%'
   and fi.dept_code='1001'
   and fi.patient_no not like '%B%'
   and fi.patient_no not like '%L%'
   

