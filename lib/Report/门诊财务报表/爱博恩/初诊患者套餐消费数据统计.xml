ypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dgvInit</Name><Sql>select * from (
select  fr.card_no 卡号,fr.name 姓名,fr.rela_phone 联系电话,
(select 
count(distinct cd.name) num from exp_package xp  inner join bd_com_package cp on xp.package_id=cp.package_id
inner join com_dictionary cd on cd.type='PACKAGETYPE' and cd.code=cp.package_kind
where  xp.card_no=fr.card_no and  xp.oper_date&gt;fr.reg_date  group by xp.card_no --,cd.name 
)  购买套餐数量,
(select 
sum(xp.real_cost ) from exp_package xp  inner join bd_com_package cp on xp.package_id=cp.package_id
inner join com_dictionary cd on cd.type='PACKAGETYPE' and cd.code=cp.package_kind
where  xp.card_no=fr.card_no and  xp.oper_date&gt;fr.reg_date   group by xp.card_no  --,cd.name   --and  xp.oper_date&gt;fr.reg_date
)  套餐消费总额
,fr.reg_date 初诊日期
  from fin_opr_register fr 
