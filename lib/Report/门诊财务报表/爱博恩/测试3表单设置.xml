       bp.package_name as 二级套餐名称,  exp.package_cost as 套餐金额 ,  exp.real_cost as 实收金额,exp.gift_cost as 优惠金额,
       vo.MEMO as 收费备注， exp.delimiting_date as 收费时间，emp.empl_name as 收费人 ,yee.empl_name  as 签单客服
  from  exp_package  exp
  left join  bd_com_package bp   on exp.package_id=bp.package_id
  left join  EXP_PACKAGEINVOICE vo on exp.invoice_no=vo.invoice_no
  left join bd_com_package bp1 on bp.parent_code=bp1.package_id
  left join  com_employee emp on  exp.delimiting_code=emp.empl_code
  left join com_employee yee on  exp.CONSULTANT=yee.empl_code
  left join com_dictionary dic on dic.code=bp.package_kind 
 where bp.package_kind in (1,3,4)
   and dic.type='PACKAGETYPE'
   and bp.valid_flag='1' and (dic.code='&amp;dtType'  or '&amp;dtType' = 'ALL')
   and bp.package_class='2' and exp.delimiting_date &gt;=to_date('&amp;dtBeginTime','yyyy-mm-dd hh24:mi:ss') and exp.delimiting_date &lt;  to_date('&amp;dtEndTime','yyyy-mm-dd hh24:mi:ss')+1 and ( patient_name  like '%&amp;dtName%' or card_no like '%&amp;dtnubm%')
 order by exp.oper_date desc</Sql><AddMapRow>false</AddMapRow><AddMapColumn>true</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\门诊财务报表\爱博恩\测试3表单设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>