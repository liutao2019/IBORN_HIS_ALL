   (case  m.agreementstatus when '1' then '暂存' when '2' then '签订' end)  agreementstatus,
   e.empl_name,
   c.name packagetype,
   p.name,   
    m.packagemoney,
    m.agreementmoney
 from Agreement m
 join com_employee e on e.empl_code  = m.clientadvisname
 join com_dictionary c on c.code = m.packagetype and type = 'APackgeType'
 join ag_com_package p on p.id = m.packagecontext
   where m.agreementstatus in('1', '2')
   and m.orderdate between '&amp;dtBeginTime' and  '&amp;dtEndTime' 
   and ( m.patientname like ('%&amp;txtXX%')  or m.patientid like Trim('%&amp;txtXX%')  or nvl('&amp;txtXX','') = '' )
   and  ('&amp;cmbName' = 'ALL'  or m.clientadvisname = '&amp;cmbName')
   order by m.orderdate</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\新客服\套餐合同查询数据.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>