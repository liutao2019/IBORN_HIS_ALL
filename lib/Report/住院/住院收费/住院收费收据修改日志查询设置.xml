       a.happen_no 发生序号,
       '收费发票',
       decode(a.tran_type, '1', '正交易', '反交易') 交易类型,
       (select C.NAME
          from com_dictionary c
         where c.Type = 'PAYMODES'
           AND C.CODE = A.OLD_PAYWAY) 原支付方式,
       (select C.NAME
          from com_dictionary c
         where c.Type = 'PAYMODES'
           AND C.CODE = A.New_Payway) 修改后支付方式,
       fun_get_employee_name(a.oper_code) 修改人,
       a.oper_date 修改时间
  from com_shiftdata_payway a
 where a.oper_date &gt;= to_date('&amp;dtBeginTime', 'yyyy-mm-dd hh24:mi:ss')
   and a.oper_date &lt; to_date('&amp;dtEndTime', 'yyyy-mm-dd hh24:mi:ss')
   and a.tran_kind = '1'
   and (a.oper_code = '&amp;dtEmployee' or '&amp;dtEmployee' = 'ALL')
