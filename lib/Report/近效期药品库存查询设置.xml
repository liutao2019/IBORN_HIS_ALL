select b.custom_code,
       b.trade_name,
       b.specs,
       p.batch_no,
       sum(round(p.store_sum/p.pack_qty,2)) ,
       p.pack_unit,
       to_char(p.valid_date,'yyyy-MM-dd'),
       p.purchase_price,
       fun_get_company_name( p.producer_code),
       p.place_code
       
from pha_com_storage p ,pha_com_baseinfo b 
where p.drug_code=b.drug_code
and p.store_sum&gt;0
and p.drug_dept_code='&amp;dtDept'
and (p.drug_code='&amp;dtDrug' or 'ALL'='&amp;dtDrug')
and p.valid_date&lt;to_date('&amp;dtBeginTime','yyyy-MM-dd')

group by b.custom_code,
       b.trade_name,
       b.specs,
       p.batch_no,
       p.pack_unit,
       to_char(p.valid_date,'yyyy-MM-dd'),
       p.purchase_price,
       fun_get_company_name( p.producer_code),
       p.place_code
order by to_char(p.valid_date,'yyyy-MM-dd')


