       f.name 姓名,
       decode(f.sex_code, 'M', '男', 'F', '女', '未知') 性别,
       f.birthday 出生日期,
       f.idenno 身份证号,
       f.address 联系地址,
       f.reg_date 挂号时间,
       f.reglevl_name 挂号级别,
       f.dept_name 挂号科室,
       f.doct_name 挂号医生,
       decode(f.ynsee, '1', '是', '否') 是否已看诊,
       f.see_date 首诊时间,
       fun_get_dept_name(f.see_dpcd) 看诊科室,
       fun_get_employee_name(f.see_docd) 看诊医生,
       decode(f.valid_flag, '1', '是', '否') 是否有效
  from fin_opr_register f
 where f.card_no = '&amp;cmbDept' 
   and f.reg_date &gt; to_date('&amp;dtBeginTime', 'yyyy-mm-dd hh24:mi:ss')
   and f.reg_date &lt;= to_date('&amp;dtEndTime', 'yyyy-mm-dd hh24:mi:ss')
