       f.dept_name 科室名称,
       to_char(f.OUT_DATE,'yyyy-mm-dd') 出院日期,
       decode(f.in_state,'N','*'||f.patient_no,f.patient_no) 住院号,
       f.NAME 姓名,
       decode(f.inpatient_no, null, null) 按时回收日,
       decode(f.inpatient_no, null, null) 延期回收日,
       decode(f.inpatient_no, null, null) 护士签名,
       decode(f.inpatient_no, null, null) 回收人员签名,
       decode(f.inpatient_no, null, null) 备注
  from FIN_IPR_INMAININFO f/*,com_department c*/,com_dictionary cc
 where f.in_state in ('O','B','N')
/*   and f.dept_code=c.dept_code*/
   and substr(f.patient_no,1,1)!='B'
   and f.OUT_DATE &gt;= to_date('&amp;dtBeginTime', 'yyyy-mm-dd hh24:mi:ss')
   and f.OUT_DATE &lt;= to_date('&amp;dtEndTime', 'yyyy-mm-dd hh24:mi:ss')
   and cc.type='DEPTBED'
   and cc.code=f.dept_code

 order by cc.input_code,f.dept_name, f.out_date</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>false</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>\Report\病案报表\出院病人清单查询设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>