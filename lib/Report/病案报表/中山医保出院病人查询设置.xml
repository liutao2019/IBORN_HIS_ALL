ource><DataSourceTypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dtDetail</Name><Sql>select i.mcard_no ,
       i.name,
       i.patient_no,
       (select c.old_data_name from com_shiftdata c where c.clinic_no=i.inpatient_no and c.shift_type='B') 入院科室,
       to_char(i.balance_date,'yyyy-MM-dd hh24:mi:ss') 出院日期,
       fun_get_dept_name(i.dept_code) 出院科室,
       nvl((select d.diag_name from met_cas_diagnose d where d.inpatient_no=i.inpatient_no and d.diag_kind=1),i.diag_name) 出院诊断,
       (select d.icd_code from met_cas_diagnose d where d.inpatient_no=i.inpatient_no and d.diag_kind=1) ICD_10,
       (select d.operation_cnname from met_cas_operationdetail d where d.inpatient_no=i.inpatient_no and d.happen_no=1) ICD_CM_3名称,
       (select d.operation_code from met_cas_operationdetail d where d.inpatient_no=i.inpatient_no and d.happen_no=1) ICD_CM_3  ,       
       i.pact_name 合同单位
from fin_ipr_inmaininfo i
 -- left join (select * from fin_ipr_siinmaininfo_zhongshan si where si.balance_no=1  )si  on si.inpatient_no=i.inpatient_no
where i.pact_code in 
      (select p.pact_code from fin_com_pactunitinfo p where p.dll_name ='ZhongShanSI.dll')
AND I.IN_STATE='O'
and i.balance_date between to_date('&amp;dtBeginTime','yyyy-MM-dd hh24:mi:ss') and to_date('&amp;dtEndTime','yyyy-MM-dd hh24:mi:ss') --出院日期
order by i.balance_date</Sql><AddMapRow>false</AddMapRow><AddMapColumn>false</AddMapColumn><AddMapData>false</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>\Report\病案报表\中山医保出院病人查询设置.xml</QueryFilePath><PrintInfo Type="FS.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>