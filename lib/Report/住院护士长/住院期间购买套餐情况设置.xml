ryDataSource><DataSourceTypeName></DataSourceTypeName><Enabled>true</Enabled></ControlType></List></List><QueryDataSource><QueryDataSource Type="Neusoft.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.QueryDataSource,SOC.HISFC.OutpatientFee.Components"><Name>dtSource</Name><Sql>select --a.card_no,count(1)
a.card_no,a.patient_no,a.name,nvl(a.home_tel,a.linkman_tel) tel,a.dept_name,a.in_date,a.out_date,bp.package_name,dp.item_name,ep.oper_date
from fin_ipr_inmaininfo a 
left join exp_package ep on a.card_no=ep.card_no 
left join bd_com_package bp on ep.package_id=bp.package_id
left join (select ep.* from exp_packagedetail ep
inner join  exp_package t  on t.clinic_code=ep.clinic_code and t.card_no=ep.card_no and t.invoice_no=ep.invoice_no and  ep.trans_type=t.trans_type 
inner join bd_com_package cp on t.package_id=cp.package_id 
where cp.package_name like '%自选包%') dp 
on ep.clinic_code=dp.clinic_code and ep.card_no=dp.card_no and ep.invoice_no=dp.invoice_no  and ep.trans_type=dp.trans_type
where  a.patient_no not like '%L%' 
and a.patient_no not like '%B%'
and a.in_state&lt;&gt;'N'
and ep.oper_date&gt;=a.in_date 
and ep.oper_date&lt;=a.out_date
and ep.pay_flag=1 and ep.cancel_flag=0
and a.in_date between to_date('&amp;dtBeginTime','yyyy-mm-dd') and to_date('&amp;dtEndTime','yyyy-mm-dd')+0.99
order by a.card_no</Sql><AddMapRow>false</AddMapRow><AddMapColumn>true</AddMapColumn><AddMapData>true</AddMapData><AddMapSourceData>false</AddMapSourceData><IsCross>false</IsCross><SqlType>MainReportUsing</SqlType><CrossRows></CrossRows><CrossColumns></CrossColumns><CrossValues></CrossValues><CrossCombinColumns></CrossCombinColumns><SumColumns></SumColumns><CrossGroupColumns></CrossGroupColumns><IsSumRow>true</IsSumRow><SumRows></SumRows><RowGroup /></QueryDataSource></QueryDataSource><QueryFilePath>Report\住院护士长\住院期间购买套餐情况设置.xml</QueryFilePath><PrintInfo Type="Neusoft.SOC.HISFC.OutpatientFee.Components.Report.Common.Setting.PrintInfo,SOC.HISFC.OutpatientFee.Components"><PaperSize Type="System.Drawing.Printing.PaperSize,System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a"><Height>1169</Height><PaperName>A4</PaperName><RawKind>0</RawKind><Width>850</Width></PaperSize><SelectPage>false</SelectPage></PrintInfo></ReportQueryInfo>