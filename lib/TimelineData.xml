          <ShowNormalRange>True</ShowNormalRange>
          <NormalRangeDescription>成人正常范围</NormalRangeDescription>
          <NormalRangeMinimumValue>60</NormalRangeMinimumValue>
          <NormalRangeMaximumValue>100</NormalRangeMaximumValue>
          <NormalRangeBrush>#FEFCE9</NormalRangeBrush>
          <HoverBackground>#656DFD</HoverBackground>
          <InterpolationLineColor>#2932E0</InterpolationLineColor>
          <DataMarkerTemplate>{StaticResource Circle}</DataMarkerTemplate>
          <Style>{StaticResource OverriddenDefaultStyle}</Style>
        </Item>
      </Row>
      <Row Id="90" Name="血压" Background="#D6EBD6">
        <Item Name="血压">
          <Units>mmHg</Units>
          <UnitsDescription>mmHg: 毫米汞柱</UnitsDescription>
          <YAxisMaxValue>300</YAxisMaxValue>
          <YAxisMinValue>0</YAxisMinValue>
          <YAxisPadding>6</YAxisPadding>
          <YAxisMajorInterval>10</YAxisMajorInterval>
          <YAxisIntervalMinimumHeight>17</YAxisIntervalMinimumHeight>
          <Height>300</Height>
          <ShowNormalRange>False</ShowNormalRange>
          <HoverBackground>#5BA25B</HoverBackground>
