          <PointTemplate>{StaticResource IBarPoint}</PointTemplate>
          <LabelTemplate>{StaticResource IBarLabel}</LabelTemplate>
          <Style>{StaticResource OverriddenDefaultStyle}</Style>
        </Item>
      </Row>
    </Section>
  </Scenario>
